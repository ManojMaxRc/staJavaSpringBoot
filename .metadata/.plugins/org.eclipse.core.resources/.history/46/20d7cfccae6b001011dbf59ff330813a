package com.training.basic;

import java.util.stream.IntStream;

public class Odd {

	public static void main(String[] args) {
		int count = 0;
		int num = 0;
		
		while (count <= 20) {
			if(num % 2 != 0) {
				System.out.print(num + " ");
				count++;
			}
			num++;			
		}
		System.out.println();
		
		IntStream.iterate(1, n -> n + 2)
		.limit(21)
		.forEach(System.out::println);
	}
}
