package com.training.basic;

import java.util.stream.IntStream;

public class Odd {

	public static void main(String[] args) {
		int count = 0;
		int num = 0;
		
		while (count <= 20) {
			if(num % 2 != 0) {
				System.out.println(num);
				count++;
			}
			num++;			
		}
		
		IntStream.range(0, 21)
		.filter(num -> num % 2 != 0 )
		.forEach(System.out::println);
	}
}
