package com.training.basic;

public class Greater {

	public static void main(String[] args) {
		int[] arr = {1, 4, 9, 114, 19, 30, 46, 59};
		
		int max = 0;
		
		for (int i = 0; i < arr.length; i++) {
			if(arr[i] > max) {
				max = arr[i];
			}
		}
		System.out.println("Greatest value : " + max);
	}
}
