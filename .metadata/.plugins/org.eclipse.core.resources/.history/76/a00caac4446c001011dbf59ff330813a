package com.training.basic;

public class Fibonacci {

	public static void main(String[] args) {
		fibonacciNumber();

	}
	
	public static void fibonacciNumber() {
		
		int first = 0;
		int second = 1;
		int temp;
		
		for (int i = 1; i < 100; i++) {
		     temp = first + second;
		     first = second;
		     second = temp;
		     System.out.println(first + " " + second);
		}
	}

}
